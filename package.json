{
  "name": "lynx-keymap",
  "displayName": "Lynx Keymap",
  "description": "Popular Lynx keybindings for Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Keymaps"
  ],
  "galleryBanner": {
    "color": "#66be8b",
    "theme": "light"
  },
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+1",
        "mac": "cmd+1",
        "command": "workbench.view.explorer"
      },
      {
        "key": "ctrl+2",
        "mac": "cmd+2",
        "command": "workbench.view.scm"
      },
      {
        "key": "ctrl+3",
        "mac": "cmd+3",
        "command": "workbench.view.extensions"
      }
    ]
  }
}
